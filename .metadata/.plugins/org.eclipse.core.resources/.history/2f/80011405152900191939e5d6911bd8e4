package org.robot.k3project.statements

import fr.inria.diverse.k3.al.annotationprocessor.Aspect
import org.robot.model.robot.Scenario
import org.robot.model.robot.statements.ControlStatement

@Aspect(className=ControlStatement)
abstract class ControlStatementAspect extends org.robot.k3project.StatementAspect {
	def Scenario exec();
}
