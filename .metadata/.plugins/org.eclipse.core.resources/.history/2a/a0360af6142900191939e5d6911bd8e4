package org.robot.k3project.statements

import fr.inria.diverse.k3.al.annotationprocessor.Aspect
import org.robot.model.robot.Scenario
import org.robot.model.robot.statements.PrintStatement

@Aspect(className=PrintStatement)
class PrintStatementAspect extends org.robot.k3project.StatementAspect {
	def Scenario exec() {
		println(_self.text)
		return null
	}
}
